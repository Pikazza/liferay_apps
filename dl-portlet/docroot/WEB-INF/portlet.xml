	<portlet>
		<portlet-name>generate-preview</portlet-name>
		<display-name>Generate Preview</display-name>
		<portlet-class>
			com.sample.portlet.GeneratePreview
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/generatepreview/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>Generate Preview</title>
			<short-title>Generate Preview</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>